            "properties": {
              "data": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/MintTicketObject"
                }
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "description": "Update the URI for the fantoken",
        "type": "object",
        "required": [
          "set_uri"
