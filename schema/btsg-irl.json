            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "description": "Update operator set for this given event",
        "type": "object",
        "required": [
          "add_epoch"
        ],
        "properties": {
          "add_epoch": {
            "type": "object",
